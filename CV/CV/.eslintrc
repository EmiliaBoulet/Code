{
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "extends": "eslint:recommended",
    "rules": {
      "indent": ["error", 2],
      "quotes": ["error", "simple"],
      "semi": ["error", "always"],
      "no-multi-spaces": "error",
      "no-trailing-spaces": "error"
    }
  }